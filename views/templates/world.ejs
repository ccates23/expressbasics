<!doctype html>
<html>
  <head>
    <title><%= title %> | Awesome Things</title>
  </head>
  <body>
    <h1>My site is <%= title %></h1>
    <h2><%= welcome %></h2>
    <h3>These are the most awesome things...</h3>
    <ul>
      <% awesomeThings.forEach(function (thing) { %>
        <li><%= thing.name %> is awesome!</li>
      <% }); %>
    </ul>
  </body>
</html>